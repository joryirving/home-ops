# Gateway WebSocket URL (required)
GATEWAY_URL=ws://localhost:18789

# Server port
PORT=3000

# Session secret (generate a random string!)
SESSION_SECRET=change-this-to-a-random-secret

# OIDC Auth (set OIDC_ENABLED=true to enable)
OIDC_ENABLED=false
OIDC_ISSUER=https://authentik.yourdomain.com
OIDC_CLIENT_ID=openclaw-chat
OIDC_CLIENT_SECRET=your-client-secret

# Local users (only used if OIDC disabled)
# Format: username:password (can have multiple, comma separated)
LOCAL_USERS=admin:password123
